	keywords = {Bycatch, Cod, Farn Deeps, Square-mesh codend, Swedish grid},
	pages = {118--125},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\UNUFTP\\Zotero\\storage\\M8EPJAS9\\Catchpole et al. - 2006 - An assessment of the Swedish grid and square-mesh .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\UNUFTP\\Zotero\\storage\\M4WIQ8V4\\S0165783606002906.html:text/html}
