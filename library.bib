@article{guttormsenOptimalManagementRenewable2008,
	title = {Optimal management of renewable resources with {Darwinian} selection induced by harvesting},
	volume = {56},
	issn = {0095-0696},
	url = {https://econpapers.repec.org/article/eeejeeman/v_3a56_3ay_3a2008_3ai_3a2_3ap_3a167-179.htm},
	abstract = {We present a bioeconomic analysis of the optimal long-term management of a genetic resource in the presence of selective harvesting. It is assumed that individuals possessing a particular gene have a lower natural mortality rate and are more valuable to capture. Highly selective harvesting may cause such a gene to lose its fitness advantage, and hence change the evolutionary path of the species. Results indicate that in a zero-cost harvesting regime, the decision to preserve the valuable gene depends on the natural rate of selection against less valuable individuals and the interest rate. On the other hand, the decision to let the less valuable gene become a significant fraction of the genes depends only on biological parameters. If marginal costs are positive, it is never optimal to let a valuable gene become extinct. Further, for some parameter values, the system exhibits multiple equilibriums. Therefore, optimal regulation may depend on initial conditions.},
	number = {2},
	urldate = {2019-12-10},
	journal = {Journal of Environmental Economics and Management},
	author = {Guttormsen, Atle and Kristofersson, Dadi and Nævdal, Eric},
	year = {2008},
	pages = {167--179},
	file = {RePEc Snapshot:C\:\\Users\\UNUFTP\\Zotero\\storage\\BAF3MBSU\\v_3a56_3ay_3a2008_3ai_3a2_3ap_3a167-179.html:text/html}
}

