	year = {2010},
	pages = {114--119},
	file = {Full Text PDF:C\:\\Users\\UNUFTP\\Zotero\\storage\\ECJFZYYD\\Duarte et al. - 2010 - Bottom Trawl Bycatch Assessment of the Shrimp Fish.pdf:application/pdf}
}

