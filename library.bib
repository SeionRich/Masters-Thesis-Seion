	pages = {566--575},
	file = {Ulrich et al. - 2017 - Achieving maximum sustainable yield in mixed fishe.pdf:C\:\\Users\\UNUFTP\\Zotero\\storage\\CJEWCEFT\\Ulrich et al. - 2017 - Achieving maximum sustainable yield in mixed fishe.pdf:application/pdf}
